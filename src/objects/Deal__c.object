    <validationRules>
        <fullName>Record_creation_with_Open_status</fullName>
        <active>true</active>
        <description>Deal__c.Status__c record can be created only with Status__c = Open</description>
        <errorConditionFormula>OR( AND
       (ISNEW(), ISPICKVAL( Status__c , &quot;Won&quot; )),
    AND
       (ISNEW(),ISPICKVAL( Status__c , &quot;Lost&quot; )))</errorConditionFormula>
        <errorMessage>Status should be Open!</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Start_Date_is_earlier_than_End_Date</fullName>
        <active>true</active>
        <description>Deal__c.Start_Date__c should not be bigger than Deal__c.End_Date__c (on Create and Edit)</description>
        <errorConditionFormula>DATETIMEVALUE(Start_Date__c) &gt;  DATETIMEVALUE( End_Date__c )</errorConditionFormula>
        <errorDisplayField>Start_Date__c</errorDisplayField>
        <errorMessage>Start Date should not be bigger than End_Date</errorMessage>
    </validationRules>
